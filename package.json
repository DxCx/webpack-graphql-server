{
  "name": "webpack-apollo-server",
  "version": "0.1.0",
  "description": "Starter kit for apollo server using webpack and typescript",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "npm install",
    "build": "webpack",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "preupver": "npm test",
    "upver": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DxCx/webpack-apollo-server.git"
  },
  "keywords": [
    "apollo",
    "backend",
    "apollo-server",
    "typescript",
    "webpack",
    "graphql",
    "graphiql",
    "express"
  ],
  "author": "Hagai Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DxCx/webpack-apollo-server/issues"
  },
  "homepage": "https://github.com/DxCx/webpack-apollo-server#readme",
  "dependencies": {
    "@types/body-parser": "0.0.33",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.31",
    "@types/hapi": "^13.0.33",
    "@types/helmet": "0.0.33",
    "@types/koa": "^2.0.31",
    "@types/morgan": "^1.7.30",
    "apollo-server": "0.3.2",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "graphql": "0.7.2",
    "graphql-tools": "0.7.2",
    "helmet": "^2.1.2",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^15.1.32",
    "@types/node": "^6.0.38",
    "awesome-typescript-loader": "^2.2.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "jest": "^16.0.2",
    "standard-version": "^3.0.0",
    "ts-jest": "^0.1.3",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typed-graphql": "^1.0.2",
    "typescript": "2.0.3",
    "typescript-babel-jest": "^0.1.2",
    "webpack": "^2.1.0-beta.21",
    "webpack-node-externals": "^1.3.3"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/typescript-babel-jest",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
